<html>
<head>
    <title>撰写文件</title>
    <link type="text/css" rel="stylesheet" href="/oa/deptfile/deptfile.css">
    <script type="text/javascript" src="/platform/commons/crud.js"></script>
    <script type="text/javascript" src="/cyan/client/validate.js"></script>
    <script type="text/javascript" src="/platform/commons/htmleditor.js"></script>
</head>
<body id="dialogBody">
    <form bind="entity" action="">
        <div id="components">
            <div class="component_item">
                <div class="component_line">
                    <div class="label">
                        文件名
                    </div>
                    <div class="component">
                        <input name="fileName">
                    </div>
                </div>
            </div>
            <div class="component_line">
                <div class="component_item">
                    <div class="label">
                        内容
                    </div>
                    <div class="component">
                        <textarea id="content" name="content" class="htmleditor"></textarea>
                    </div>
                </div>
            </div>
        </div>
        <div id="buttons">
            <span class="button">
                <button type="button" onclick="save();">保存</button>
            </span>
            <span class="button">
                <button type="button" onclick="closeWindow();">关闭</button>
            </span>
        </div>

    </form>
</body>
</html>