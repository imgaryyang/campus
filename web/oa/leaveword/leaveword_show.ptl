<html>
<head>
    <title></title>
    <link type="text/css" rel="stylesheet" href="/oa/leaveword/leaveword_show.css">
    <script type="text/javascript" src="/platform/commons/crud.js"></script>
</head>

<body id="dialogBody">
    <div id="main">
        <form id="form" bind="entity" action="">

            <div class="components">
                <div class="component_line">
                    <div class="component_item">
                        <div class="label">
                            标题
                        </div>
                        <div class="component">${title}</div>
                    </div>
                </div>
            </div>

            <div class="components">
                <div class="component_line">
                    <div class="component_item">
                        <div class="label">
                            留言时间
                        </div>
                        <div class="component">${leaveTime}</div>
                    </div>
                </div>
            </div>

            <div class="components">
                <div class="component_line">
                    <div class="component_item">
                        <div class="label">
                            留言内容
                        </div>
                        <div class="content">${content}</div>
                    </div>
                </div>
            </div>

            <div id="buttons">
                <span class="button">
                    <button type="button" onclick="closeWindow();">关闭</button>
                </span>
            </div>

        </form>
    </div>
</body>
</html>