<html>
<head>
    <title>视频上传</title>
    <link type="text/css" rel="stylesheet" href="/platform/styles/big.css">
    <link type="text/css" rel="stylesheet" href="/platform/attachment/video.css">
    <script type="text/javascript" src="/cyan/client/validate.js"></script>
    <script type="text/javascript" src="/platform/attachment/video.js"></script>
</head>
<body id="dialogBody">

    <form id="form" action="">
        <div id="components">
            <div class="component_item">
                <div class="label">视频</div>
                <div class="component">
                    <input type="file" name="file" require>
                </div>
            </div>
        </div>
        <div id="size">
            <span>
                宽度:<input name="width" value="480">
            </span>
            <span>
                高度:<input name="height" value="320">
            </span>
        </div>
        <div id="buttons">
            <span class="button">
                <button type="button" onclick="ok();" progress="true">确定</button>
            </span>
            <span class="button">
                <button type="button" onclick="cancel();">取消</button>
            </span>
        </div>
    </form>
</body>
</html>