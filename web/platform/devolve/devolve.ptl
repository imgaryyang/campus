<html>
<head>
    <script type="text/javascript" src="/cyan/client/validate.js"></script>
    <script type="text/javascript" src="/platform/devolve/devolve.js"></script>
    <link type="text/css" rel="stylesheet" href="/platform/devolve/devolve.css">
</head>
<body id="dialogBody">

    <form id="form">

        <div id="components">
            <div class="component_line" bind="fromUserId==null">
                <div class="component_item">
                    <div class="label">移交人</div>
                    <div class="component">
                        <input name="fromUserId">
                    </div>
                </div>
            </div>
            <div class="component_line">
                <div class="component_item">
                    <div class="label">移交给</div>
                    <div class="component">
                        <input name="toUserId">
                    </div>
                </div>
            </div>
            <div class="component_line">
                <div class="component_item">
                    <div class="label">开始时间</div>
                    <div class="component">
                        <input name="startTime">
                    </div>
                </div>
            </div>
            <div class="component_line">
                <div class="component_item">
                    <div class="label">结束时间</div>
                    <div class="component">
                        <input name="endTime">
                    </div>
                </div>
            </div>
            <div class="component_line">
                <div class="component_item">
                    <div class="label">移交内容</div>
                    <div class="component">
                        <span bind="items" class="checkbox">
                            <input type="checkbox" class="checkbox" name="ids" value="${id}">${name}
                        </span>
                    </div>
                </div>
            </div>
        </div>
        <div id="buttons">
            <span class="button">
                <button onclick="ok()" type="button">确定</button>
            </span>
            <span class="button">
                <button onclick="cancel()" type="button">取消</button>
            </span>
        </div>

    </form>
</body>
</html>