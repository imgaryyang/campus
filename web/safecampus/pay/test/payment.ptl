
<div class="container">
    <div class="row clearfix">
        <div class="col-md-12 column">
            <table class="table table-hover table-bordered  table-striped" >
                <thead>
                <tr>
                    <th>
                        项目
                    </th>
                    <th>
                        结果
                    </th>
                </tr>
                </thead>
                <tbody bind="payment">
                <tr><td>账单状态</td><td>${billStatus}</td></tr>
                <tr><td>支付完成时间</td><td>${payTime}</td></tr>
                <tr><td>关联账单</td><td>${billId}</td></tr>
                <tr><td>关联缴费项目</td><td>${payItem.payItemName}-${payItemId}</td></tr>
                <tr><td>关联学生</td><td>${student.studentName}-${studentId}</td></tr>
                <tr><td>分行-分行号</td><td>${branchNo}-${merchantNo}</td></tr>
                <tr><td>发起时间</td><td>${createTime}</td></tr>
                <tr><td>用户协议号</td><td>${agrNo}</td></tr>
                <tr><td>用户签约流水号</td><td>${merchantSerialNo}</td></tr>
                <tr><td>备注</td><td>${note}</td></tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
<script src="https://code.jquery.com/jquery.js"></script>
<script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>